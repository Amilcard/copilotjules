# PostgreSQL Connection Details
DB_USER=your_db_user
DB_HOST=localhost
DB_DATABASE=dune_db
DB_PASSWORD=your_db_password
DB_PORT=5432

# Server Configuration
PORT=3001
FRONTEND_URL=http://localhost:5173 # Adjust if your frontend runs on a different port

# JWT Secret (IMPORTANT: Change this to a long, random string for production!)
JWT_SECRET=yourrandomsecretkeychangethis!

# Google OAuth Credentials
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
# Ensure this callback URL matches exactly what's in your Google Cloud Console
# And that it points to your backend's Google callback route (e.g., /api/auth/google/callback)
GOOGLE_CALLBACK_URL=http://localhost:3001/api/auth/google/callback 

# Facebook OAuth Credentials
FACEBOOK_APP_ID=your_facebook_app_id
FACEBOOK_APP_SECRET=your_facebook_app_secret
# Ensure this callback URL matches exactly what's in your Facebook Developer Console
# And that it points to your backend's Facebook callback route (e.g., /api/auth/facebook/callback)
FACEBOOK_CALLBACK_URL=http://localhost:3001/api/auth/facebook/callback
